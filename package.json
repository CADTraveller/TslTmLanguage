{
    "name": "tsl",
    "displayName": "TSL Colors",
    "description": "VS Code Language support for Tsl Scripts (.mcr)",
    "version": "0.7.0",
    "publisher": "cvcolomb",
    "icon": "images/hsbHouse.png",
    "galleryBanner": {
        "color": "#016315",
        "theme": "dark"
    },
    "homepage": "https://github.com/CADTraveller/TslTmLanguage/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/CADTraveller/TslTmLanguage.git"
    },
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages",
        "Themes"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tsl",
                "aliases": [
                    "Tsl",
                    "tsl"
                ],
                "extensions": [
                    "mcr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsl",
                "scopeName": "source.tsl",
                "path": "./syntaxes/tsl.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "TslColors",
                "uiTheme": "hc-black",
                "path": "./themes/TslColors.tmTheme"
            }
        ]
    }
}